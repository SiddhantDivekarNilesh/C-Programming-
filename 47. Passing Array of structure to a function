//program :passing array of structures to functions
#include<stdio.h>
struct Stud
{
    int id;
    char name[20];
};
void display(struct Stud s[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("ID: %d  Name: %s\n",s[i].id,s[i].name);
    }
}
int main()
{
    struct Stud s[3]={{101,"Siddhant"},{102,"Shreyas"},{103,"Giridhar"}};
    display(s,3);
    return 0;
}