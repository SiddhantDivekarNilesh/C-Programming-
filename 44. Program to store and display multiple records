//Program to store and display multiple student records using array of structures
#include<stdio.h>
struct Student
{
    int id;
    char name[20];
};
int main()
{
    struct Student s[2]={{101,"Siddhant"},{102,"Anushka"}};
    for(int i=0;i<2;i++)
    {
        printf("ID: %d\nName: %s\n",s[i].id,s[i].name);
    }
    return 0;
}