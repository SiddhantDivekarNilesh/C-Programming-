//linear search
#include<stdio.h>
int main()
{
    int arr[5]={10,25,30,45,50};
    int i,key,found=0;
    printf("Enter the element to be searched");
    scanf("%d",&key);
    for(i=0;i<5;i++)
    {
        if(arr[i]==key)
        {
            found=1;
            break;
        }
    }
    if(found==1)
    {
        printf("The Element %d is found at position %d",key,i+1);
    }
    else 
        printf("The Element %d is not found",key);

    return 0;
}
